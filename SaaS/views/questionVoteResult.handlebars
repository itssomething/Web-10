<div>
  <h3>{{total}} vote</h3>
  <div class="progress">
    <div class="progress-bar progress-bar-danger" style="width: {{yes}}%">
      <i class="fa fa-thumbs-o-down" aria-hidden="true"></i> {{yes}}%
    </div>
    <div class="progress-bar progress-bar-primary" style="width: {{no}}%">
      <i class="fa fa-thumbs-o-up" aria-hidden="true"></i> {{no}}%
    </div>
  </div>
</div>